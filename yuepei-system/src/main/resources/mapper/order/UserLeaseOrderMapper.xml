    <select id="userLeaseOrder" resultMap="UserLeaseOrderResult">
        select a.*,b.rule from user_lease_order a,device_rule b
        <where>
            a.openid = #{openid}
            and a.device_number=b.device_number
            <if test="status != null">and a.status = #{status}</if>
        </where>
